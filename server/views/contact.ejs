  <div class="row justify-content-md-center" id="contact1" style="color:#453D30; font-family: Garamond;">
    <div class="col-lg-4 container">
        <form>
        <div class="form-group">
        <label for="fullName">Full Name</label>
        <input id ="contact3" type="text" class="form-control" id="fullName" aria-describedby="fullName" name="fullName" placeholder="Enter your full name." required>
        </div>
        <div class="form-group">
        <label for="contactNumber">Contact Number</label>
        <input id ="contact3" type="tel" class="form-control" id="contactNumber" aria-describedby="contactNumber" name="contactNumber" placeholder="Enter your contact number." required>
        </div>
        <div class="form-group">
        <label for="emailAddress">Email address</label>
        <input id ="contact3"  type="email" class="form-control" id="emailAddress" aria-describedby="emailAddress" name="emailAddress" placeholder="Enter your e-mail address." required>
        </div>
        <div class="form-group">
        <label for="message">Your Message:</label>
        <textarea id ="contact3" class="form-control" id="message" name="message" rows="5" placeholder="Ask anything you want to ask."></textarea>
        </div>
        <div class="row justify-content-around ">
        <button id="btnColor" type="submit" class="btn background1 col-3"><h5>Send</h5></button>
        <a class="btn background1 col-3" id="btnColor" href="/home">Cancel</a>
        </div>
        </form>
    </div>
