<%- include partials/header.ejs %>
<%- include partials/main_nav.ejs %>


<div id="w5">
    <h1 class="position-relative p-3 p-md-1 m-md-5 text-center background4 text1" style="color:#453D30; font-family: Garamond;">Message List</h1>
      <div class="container">
          
            
        <% for(let count = 0; count < MessageList.length; count++) { %>
        <div class="card" style="width: 18rem;">
          <div class="card-body">
            <h5 class="card-title"><%= count + 1 %>. <%= MessageList[count].fullName %></h5>
            <h6 class="card-subtitle mb-2 text-muted"><%= MessageList[count].contactNumber %></h6>
            <h6 class="card-subtitle mb-2 text-muted"><%= MessageList[count].emailAddress %></h6>
            <p class="card-text"><%= MessageList[count].message %></p>
            <a href="/message/delete/<%= MessageList[count]._id %>" class="btn btn-outline-danger btn-sm" role="button"><i class="far fa-trash-alt"></i> Delete </a>
          </div>
        </div>
        <br>
        <% } %>
    

      </div>
</div>

<%- include partials/bottom_nav.ejs %>
<%- include partials/footer.ejs %>