<%- include partials/header.ejs %>
<%- include partials/main_nav.ejs %>

  <div id="w5">
  <div style="background-image: url('../../Assets/images/board1.png'); margin: 80px; background-size: contain; border-radius: 100px;">
      <br>
      <h1 class="display-3 position-relative p-3 p-md-1 m-md-5 text-center background4 text1" style="color:#2e2d2c; font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif;"><b>Message List</b></h1>
        <div class="row justify-content-md-center">

          <ul class="ulInMessageforstyle ulforOverflowPadding">
            <% for(let count = 0; count < MessageList.length; count++) { %>
              <li class="liInMessageforstyle liformarginFloat liAddproperties">
                <a href="/message/delete/<%= MessageList[count]._id %>" class="aInMessage aforBackground aRotate aHover aFocus aTransition aColar">
                  <h2 class="h2InMessage h2forPB"><%= count + 1 %>. <%= MessageList[count].fullName %></h2>
                  <p class="pInMessage pforFontAndSize"><%= MessageList[count].message %></p>
                  <p class="btn btn-outline-danger btn-sm" role="button"><i class="far fa-trash-alt"></i> Delete </p> 
                </a>
              </li>
            <% } %>
            
          </ul>

        
      
        </div>
        </div>
  </div>
